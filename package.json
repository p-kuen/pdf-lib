  "scripts": {
    "release:latest": "yarn publish --tag latest && yarn pack && yarn release:tag",
    "release:next": "yarn publish --tag next",
    "release:prep": "yarn clean && yarn lint && yarn typecheck && yarn test && yarn build",
    "release:tag": "TAG=\"v$(yarn --silent get:version)\" && git tag $TAG && git push origin $TAG",
    "get:version": "node --eval 'console.log(require(`./package.json`).version)'",
    "clean": "rimraf -g build dist es scratchpad/build coverage tsBuildInfo.json apps/node-build apps/node/tsBuildInfo.json isolate*.log flamegraph.html out.pdf",
    "typecheck": "tsc --noEmit --incremental false --tsBuildInfoFile null",
    "test": "vitest run",
    "testw": "vitest --watch",
    "testc": "vitest --coverage && open coverage/index.html",
    "lint": "yarn lint:prettier && yarn lint:tslint:src && yarn lint:tslint:tests",
    "lint:tslint:src": "tslint --project tsconfig.json --fix",
    "lint:tslint:tests": "tslint --project tests/tsconfig.json --fix",
    "lint:prettier": "prettier --write \"./{src,tests,apps}/**/*.{ts,js,json,html,css}\" --log-level error",
    "build": "yarn build:esm && yarn build:bundle && yarn build:bundle:min",
    "build:esm": "tsc --outDir es",
    "build:bundle": "rollup --config rollup.config.mjs --file dist/pdf-lib.js --environment MODULE_TYPE:es",
    "build:bundle:min": "rollup --config rollup.config.mjs --file dist/pdf-lib.min.js --environment MINIFY,MODULE_TYPE:es",
    "scratchpad:start": "tsc --build scratchpad/tsconfig.json --watch",
    "scratchpad:run": "node scratchpad/build/scratchpad/index.js",
    "scratchpad:flame": "rimraf --glob isolate*.log && node --prof scratchpad/build/scratchpad/index.js && node --prof-process --preprocess -j isolate*.log | flamebearer",
    "apps:node": "tsc --build apps/node/tsconfig.json && node apps/node-build/index.js",
    "apps:deno": "deno run --allow-read --allow-write --allow-run apps/deno/index.ts",
    "apps:web": "http-server -c-1 .",
    "apps:web:mac": "bash -c 'sleep 1 && open http://localhost:8080/apps/web/test1.html' & yarn apps:web",
    "apps:rn:ios": "cd apps/rn && yarn add ./../.. --force && react-native run-ios",
    "apps:rn:android": "yarn apps:rn:emulator & cd apps/rn && yarn add ./../.. --force && react-native run-android",
    "apps:rn:emulator": "emulator -avd \"$(emulator -list-avds | head -n 1)\" & bash -c 'sleep 5 && adb reverse tcp:8080 tcp:8080 && adb reverse tcp:8081 tcp:8081'"
  },
  "module": "es/index.js",
  "unpkg": "dist/pdf-lib.min.js",
  "types": "es/index.d.ts",
  "type": "module",
  "exports": "./es/index.js",
  "files": [
    "dist/",
    "es/",
    "src/",
    "LICENSE.md",
    "package.json",
    "README.md",
    "yarn.lock"
  ],
  "dependencies": {
    "@patcher56/upng": "^2.0.0-beta.0",
    "@pdf-lib/standard-fonts": "^1.0.0",
    "pako": "^2.1.0"
  },
  "devDependencies": {
    "@pdf-lib/fontkit": "^1.1.1",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/pako": "^2.0.3",
    "@vitest/coverage-v8": "^1.4.0",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "flamebearer": "^1.1.3",
    "http-server": "^14.1.1",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "rollup": "^3.29.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.4.4",
    "vitest": "^1.4.0"
  },
  "license": "MIT",
  "homepage": "https://pdf-lib.js.org",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/p-kuen/pdf-lib.git"
  },
  "bugs": {
    "url": "https://github.com/p-kuen/pdf-lib/issues"
  },
  "keywords": [
    "pdf-lib",
    "pdf",
    "document",
    "create",
    "modify",
    "creation",
    "modification",
    "edit",
    "editing",
    "typescript",
    "javascript",
    "library"
  ],
  "directories": {
    "doc": "docs",
    "test": "tests"
  }
}
